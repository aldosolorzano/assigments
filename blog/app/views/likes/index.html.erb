
  <div class="container">
    <h1>Liked posts</h1>
    <% @posts.each do |p| %>
    <div class="post-text">
      <h3><%= link_to p.title, post_path(p) %></h3>
      <p><%=p.body%></p>
      <%= link_to p.user.full_name %>
      <p><%= p.created_at.strftime("%Y-%m-%d")%></p>
    </div>
    <hr>
    <% end %>
</div>
